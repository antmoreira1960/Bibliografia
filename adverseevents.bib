  keywords           = {Acupuncture Therapy/*adverse effects, Adolescent, Adult, Aged, Female, Humans, Male, Middle Aged, Pneumothorax/diagnosis/*etiology/surgery, Retrospective Studies, Thoracostomy, Young Adult},
  language           = {eng},
  linking-issn       = {0360-1293},
  nlm-unique-id      = {7610364},
  owner              = {NLM},
  print-issn         = {0360-1293},
  publication-status = {ppublish},
