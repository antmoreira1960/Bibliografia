  issue                  = {6},
  keywords               = {*Acupuncture Points, Acupuncture Therapy/*adverse effects, Adult, Female, Humans, Male, *Pneumothorax/etiology/prevention & control, Reference Values, *Ultrasonography, GB 21, acupuncture, complication, pneumothorax, ultrasonography},
