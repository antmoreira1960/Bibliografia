  grantno                = {Canadian Institutes of Health Research/Canada},
  groups                 = {Pneumo Case Report},
  history                = {2012/01/25 06:00 [medline]},
  issue                  = {6},
  keywords               = {Acupuncture Therapy/*adverse effects, Adolescent, Child, Child, Preschool, Humans, Infant, Infant, Newborn, Severity of Illness Index},
  language               = {eng},
  linking-issn           = {0031-4005},
  location-id            = {10.1542/peds.2011-1091 [doi]},
  nlm-unique-id          = {0376422},
  owner                  = {NLM},
  publication-status     = {ppublish},
  revised                = {20181201},
  source                 = {Pediatrics. 2011 Dec;128(6):e1575-87. doi: 10.1542/peds.2011-1091. Epub 2011 Nov 21.},
  status                 = {MEDLINE},
  subset                 = {IM},
  title-abbreviation     = {Pediatrics},
}

