  issue              = {4},
  keywords           = {Acupuncture, Body Mass Index, Pneumothorax, Risk Factors},
  language           = {eng},
  linking-issn       = {1933-6586},
  nlm-unique-id      = {100899009},
  owner              = {NLM},
  print-issn         = {1933-6586},
  publication-status = {ppublish},
  revised            = {20181113},
  source             = {Med Acupunct. 2014 Aug 1;26(4):241-245. doi: 10.1089/acu.2013.1022.},
  status             = {Publisher},
  termowner          = {NOTNLM},
  title-abbreviation = {Med Acupunct},
}

